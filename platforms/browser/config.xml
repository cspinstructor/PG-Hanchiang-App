<?xml version='1.0' encoding='utf-8'?>
<widget id="pg.adv.panel" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PG Admob Advanced Panel</name>
    <description>
        A test of Admob on JQuery Phone Gap with ajax panels
    </description>
    <author email="ezipicapp@gmail.com" href="https://ezipicapp.blogspot.com">
        Paul
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon144.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
